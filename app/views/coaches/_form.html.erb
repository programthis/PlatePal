<%= form_for(@coach) do |f| %>
	<p>
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :email %><br>
		<%= f.text_field :email %>
	</p>
	<p>
		<%= f.label :password %><br>
		<%= f.password_field :password %>
	</p>
	<p>
		<%= f.label :password_confirmation %><br>
		<%= f.password_field :password_confirmation %>
	</p>
	<p>
		<%= f.label :avatar %><br>
		<%= f.text_field :avatar %>
	</p>
	<p>
		<%= f.submit "Sign up" %>
	</p>
<% end %>