<h1>Profile</h1>

Name: <%= @patient.name %><br>
Email: <%= @patient.email %><br>
Weight: <%= @patient.weight %><br>
Height: <%= @patient.height %><br>
Phone: <%= @patient.phone %><br>

<% @patient.photos.each do |photo| %>
	<%= photo.name %><br>
	<%= photo.notes %><br>
	<%= link_to image_tag("#{photo.url}"),photo_path(photo) %>
<% end %>